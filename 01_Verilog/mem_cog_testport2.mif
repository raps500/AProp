-- Port A test, toggles a PIN
-- using waitcnt
WIDTH=32;
DEPTH=8;

ADDRESS_RADIX=HEX;
DATA_RADIX=BIN;

CONTENT BEGIN
--   OOOOOOZCRiCCCCDDDDDDDDDSSSSSSSSS
00 : 10100000111111111110100000000011; -- mov dira, #3
01 : 10100000101111111100000111110001; -- mov temp, CNT ($1E0)
02 : 10000000111111111100000000100000; -- add temp, #32
03 : 11111000111111111100000000010100; -- waitcnt temp, #20
04 : 10100000111111111110110000000010; -- mov porta, #2
05 : 11111000111111111100000000010100; -- waitcnt temp, #20
06 : 10100000111111111110110000000001; -- mov porta, #1
07 : 01011100011111000000000000000011; -- jmp #3

END;
